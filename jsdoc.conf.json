{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "./README.md"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "useShortNamesInLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "collalpse": true,
    "search": true,
    "menu":{
      "Github Repo":{
        "href":"https://github.com/multiparty/jigg",
        "target":"_blank",
        "class":"menu-item",
        "id":"github_link"
      },
      "Tutorial":{
        "href":"tutorial.html",
        "class":"menu-item"
      }
    },
    "meta": {
      "title": "JIGG Library Documentation"
    }
  }
}
